{
  "name": "jdn-ai-chrome-extension",
  "version": "3.5.314",
  "description": "jdn-ai chrome extension",
  "scripts": {
    "start": "webpack --watch --env devenv",
    "start:prodenv": "webpack --watch",
    "start:reduxlog": "webpack --watch --env reduxlogenable devenv",
    "build": "webpack --mode production",
    "build:devenv": "webpack --mode production --env devenv",
    "patch": "npx mversion patch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .jsx --ext .tsx --ext .ts --ext .js src/",
    "lint:fix": "eslint --ext .jsx  --ext .tsx --ext .ts --ext .js src/ --fix",
    "prettier": "prettier --write src/**/*.jsx",
    "code:fix": "npm run lint:fix && npm run prettier"
  },
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdi-testing/jdn-ai.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "JDI team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdi-testing/jdn"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint"
    }
  },
  "modulePaths": [
    "<rootDir>"
  ],
  "type": "module",
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@react-hook/resize-observer": "^1.2.6",
    "@reduxjs/toolkit": "1.7.2",
    "antd": "4.24.3",
    "axios": "0.27.2",
    "cyrillic-to-translit-js": "3.1.0",
    "jsdom": "19.0.0",
    "jszip": "3.7.1",
    "lodash": "4.17.21",
    "mversion": "2.0.1",
    "phosphor-react": "1.4.1",
    "react": "17.0.2",
    "react-children-utilities": "^2.8.0",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-remark": "^2.1.0",
    "redux-undo": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-decorators": "7.17.2",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "6.2.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/chrome": "^0.0.193",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-jest": "^28.0.0",
    "babel-loader": "8.2.3",
    "css-loader": "5.2.7",
    "eslint": "^7.32.0",
    "eslint-config-google": "0.14.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "file-loader": "6.2.0",
    "file-saver": "2.0.5",
    "html-webpack-plugin": "5.5.0",
    "husky": "^8.0.2",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-transform-stub": "2.0.0",
    "jest-webextension-mock": "^3.7.22",
    "json-loader": "0.5.7",
    "less": "4.1.2",
    "less-loader": "8.1.1",
    "mini-css-extract-plugin": "1.6.2",
    "prettier": "2.2.1",
    "prettier-eslint": "^15.0.1",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "17.0.2",
    "style-resources-loader": "1.5.0",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "5.72.1",
    "webpack-cli": "4.9.0"
  }
}
