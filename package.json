{
  "name": "jdn-ai-chrome-extension",
  "version": "3.15.1",
  "description": "jdn-ai chrome extension",
  "scripts": {
    "start": "webpack --watch --env devenv",
    "start:prodenv": "webpack --watch",
    "start:reduxlog": "webpack --watch --env reduxlogenable devenv",
    "build": "webpack --mode production",
    "build:devenv": "webpack --mode production --env devenv",
    "patch": "node ./patch.js && git add manifest.json package.json package-lock.json && git commit -m \"update version\"",
    "minor": "npx mversion minor",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .jsx --ext .tsx --ext .ts --ext .js src/",
    "lint:fix": "eslint --ext .jsx  --ext .tsx --ext .ts --ext .js src/ --fix",
    "prettier": "prettier --write src/**/*.jsx",
    "code:fix": "npm run lint:fix && npm run prettier",
    "postinstall": "patch-package"
  },
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/jdi-testing/jdn-ai.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "JDI team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdi-testing/jdn"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint"
    }
  },
  "modulePaths": [
    "<rootDir>"
  ],
  "type": "module",
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@medv/finder": "^3.1.0",
    "@phosphor-icons/react": "^2.0.10",
    "@react-hook/resize-observer": "^1.2.6",
    "@reduxjs/toolkit": "1.9.5",
    "@sindresorhus/transliterate": "1.6.0",
    "@types/file-saver": "^2.0.5",
    "file-saver": "2.0.5",
    "antd": "^5.11.5",
    "axios": "^1.6.2",
    "css-selector-generator": "^3.6.4",
    "jszip": "^3.10.1",
    "lodash": "4.17.21",
    "rc-field-form": "^1.40.0",
    "react": "18.2.0",
    "react-children-utilities": "^2.8.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2",
    "react-remark": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "redux-undo": "^1.0.1",
    "rehype-raw": "^6.1.1",
    "rehype-sanitize": "^5.0.1",
    "remark-emoji": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.11",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-decorators": "7.22.10",
    "@babel/plugin-transform-runtime": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "^7.18.6",
    "@svgr/webpack": "8.1.0",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/chrome": "^0.0.233",
    "@types/jest": "^28.1.6",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "babel-jest": "^29.6.4",
    "babel-loader": "9.1.3",
    "css-loader": "6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.6.4",
    "jest-transform-stub": "2.0.0",
    "jest-webextension-mock": "3.7.22",
    "jsdom": "^21.1.1",
    "json-loader": "0.5.7",
    "less": "4.2.0",
    "less-loader": "11.1.3",
    "mini-css-extract-plugin": "2.7.6",
    "patch-package": "^6.5.1",
    "prettier": "3.0.3",
    "prettier-eslint": "^15.0.1",
    "raw-loader": "^4.0.2",
    "react-test-renderer": "18.2.0",
    "style-resources-loader": "1.5.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.83.1",
    "webpack-cli": "5.1.4"
  }
}